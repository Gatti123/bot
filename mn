import aiogram
from aiogram import types
from aiogram.types import InlineKeyboardButton, InlineKeyboardMarkup, InputFile

bot = aiogram.Bot(token="6224012362:AAHe8EesEmYEjIaFpJZ1TxthUsb-KF9r3PM")
dp = aiogram.Dispatcher(bot)

# –ü—É—Ç–∏ –∫ PDF —Ñ–∞–π–ª–∞–º
pdf_files = {
    "üí≤ –î–æ $300,000": "/home/tim1212/bot/catalogs/catalog-mira-estate 300.pdf",
    "üí≤ –î–æ $750,000": "/home/tim1212/bot/catalogs/catalog-mira-estat750.pdf",
    "üí≤ –û—Ç $1,000,000": "/home/tim1212/bot/catalogs/catalog-mira-estate 1000000.pdf",
    "üè° –í–∏–ª–ª–∞/–¢–∞—É–Ω—Ö–∞—É—Å": "/home/tim1212/bot/catalogs/catalog-mira-estate villas.pdf"
}

@dp.message_handler(commands=['start'])
async def start(message: types.Message):
    markup1 = types.ReplyKeyboardMarkup(row_width=2, resize_keyboard=True)
    button1 = types.KeyboardButton("üè† –ö—É–ø–∏—Ç—å")
    button2 = types.KeyboardButton("üí∞ –ü—Ä–æ–¥–∞—Ç—å")
    button3 = types.KeyboardButton("‚ÑπÔ∏è –ü—Ä–æ –î—É–±–∞–π")
    button4 = types.KeyboardButton("üë®‚Äçüíº –°–≤—è–∑–∞—Ç—å—Å—è —Å —ç–∫—Å–ø–µ—Ä—Ç–æ–º")
    button5 = types.KeyboardButton("üì¨ –ü–æ–¥–ø–∏—Å–∞—Ç—å—Å—è –Ω–∞ —Ä–∞—Å—Å—ã–ª–∫—É")
    markup1.add(button1, button2, button3, button4, button5)
    await message.answer("–ü—Ä–∏–≤–µ—Ç! –Ø –±–æ—Ç.", reply_markup=markup1)

@dp.message_handler(content_types=types.ContentTypes.TEXT)
async def handle_buttons(message: types.Message):
    if message.text == "üè† –ö—É–ø–∏—Ç—å":
        markup = types.ReplyKeyboardMarkup(row_width=1, resize_keyboard=True)
        button1 = types.KeyboardButton("üè¢ –ê–ø–∞—Ä—Ç–∞–º–µ–Ω—Ç")
        button2 = types.KeyboardButton("üè° –í–∏–ª–ª–∞/–¢–∞—É–Ω—Ö–∞—É—Å")
        button3 = types.KeyboardButton("üîç –ü–µ—Ä—Å–æ–Ω–∞–ª—å–Ω–∞—è –ø–æ–¥–±–æ—Ä–∫–∞")
        button_back = types.KeyboardButton("üîô –ù–∞–∑–∞–¥")
        markup.add(button1, button2, button3, button_back)
        await message.answer("–í—ã–±–µ—Ä–∏—Ç–µ —Ç–∏–ø –Ω–µ–¥–≤–∏–∂–∏–º–æ—Å—Ç–∏:", reply_markup=markup)

    elif message.text == "üè¢ –ê–ø–∞—Ä—Ç–∞–º–µ–Ω—Ç":
        markup = types.ReplyKeyboardMarkup(row_width=1, resize_keyboard=True)
        button1 = types.KeyboardButton("üí≤ –î–æ $300,000")
        button2 = types.KeyboardButton("üí≤ –î–æ $750,000")
        button3 = types.KeyboardButton("üí≤ –û—Ç $1,000,000")
        button_back = types.KeyboardButton("üîô –ù–∞–∑–∞–¥")
        markup.add(button1, button2, button3, button_back)
        await message.answer("–í—ã–±–µ—Ä–∏—Ç–µ –±—é–¥–∂–µ—Ç:", reply_markup=markup)

    elif message.text in pdf_files:
        pdf_file_path = pdf_files[message.text]
        await message.answer_document(types.InputFile(pdf_file_path), caption="–í–∞—à –∫–∞—Ç–∞–ª–æ–≥, –¥–ª—è –±–æ–ª–µ–µ –¥–µ—Ç–∞–ª—å–Ω–æ–π –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ –∏ –∏–Ω–¥–∏–≤–∏–¥—É–∞–ª—å–Ω–æ–≥–æ –ø–æ–¥–±–æ—Ä–∞, –æ–±—Ä–∞—Ç–∏—Ç–µ—Å—å –∫ –Ω–∞—à–∏–º —ç–∫—Å–ø–µ—Ä—Ç–∞–º")

    elif message.text == "üè° –í–∏–ª–ª–∞/–¢–∞—É–Ω—Ö–∞—É—Å":
        # –û—Ç–ø—Ä–∞–≤–ª—è–µ–º –ø–¥—Ñ —Ñ–∞–π–ª
        with open(pdf_file_path, "rb") as file:
            pdf_file = InputFile(file)
            await message.answer_document(pdf_file, caption="–í–∞—à –∫–∞—Ç–∞–ª–æ–≥ —Å –í–∏–ª–ª–∞–º–∏ –∏ –¢–∞—É–Ω—Ö–∞—É—Å–∞–º–∏. –î–ª—è –±–æ–ª–µ–µ –¥–µ—Ç–∞–ª—å–Ω–æ–π –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏ –æ–±—Ä–∞—Ç–∏—Ç–µ—Å—å –∫ –Ω–∞—à–∏–º —ç–∫—Å–ø–µ—Ä—Ç–∞–º")

